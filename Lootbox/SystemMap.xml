<?xml version="1.0" encoding="utf-8"?>

<TranscendenceModule>

	<!-- A single system map. -->

	<SystemMap UNID="&ddddMapLootbox;"
			name=				"Sandbox Space"
			backgroundImage=	""
			initialScale=		"100"
			minScale=			"50"
			maxScale=			"100"
			>

		<!-- One system per lootbox method here -->
		<TopologyCreator>
			<Stargate from="SystemA:Outbound" to="SystemA:Inbound"/>
            <Stargate from="SystemB:Outbound" to="SystemB:Inbound" />
		</TopologyCreator>

		<Node ID="SystemA" x="0" y="0">
			<System UNID=		"&ddddSysSystemA;"
					name=		"SystemA"
					level=		"1"
					attributes=	"sandboxSpace"
					/>
		</Node>

        <Node ID="SystemB" x="0" y="0">
            <System UNID=		"&ddddSysSystemB;"
                    name=		"SystemB"
                    level=		"1"
                    attributes=	"sandboxSpace"
            />
        </Node>
	</SystemMap>

</TranscendenceModule>
